{
  "private": true,
  "scripts": {
    "build": "npm run build:dockpanel && npm run build:flexbox && npm run build:menus && npm run build:tabs && npm run build:todomvc",
    "build:dockpanel": "tsc --project dockpanel && webpack --config dockpanel/webpack.conf.js",
    "build:flexbox": "tsc --project flexbox && webpack --config flexbox/webpack.conf.js",
    "build:menus": "tsc --project menus && webpack --config menus/webpack.conf.js",
    "build:tabs": "tsc --project tabs && webpack --config tabs/webpack.conf.js",
    "build:todomvc": "tsc --project todomvc && webpack --config todomvc/webpack.conf.js"
  },
  "dependencies": {
    "css-loader": "^0.23.1",
    "phosphor": "^0.6.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.13"
  },
  "devDependencies": {
    "rimraf": "^2.4.3",
    "typescript": "^1.6.2"
  }
}
